---
---
{ {% for post in site.posts %}
  "{{post.url}}": {
    "url": "{{post.url}}",
    "tags": "{{post.tags | join: ', '}}",
    "category": "{{site.post_categories[post.category]['name']}}",
    "title": "{{post.title | escape}}",
    "excerpt": "{{post.excerpt | strip_html | strip_newlines}}",
    "date": "{{post.date | date: "%d/%m/%Y"}}"
  } {% if post.previous %},{% endif %} {% endfor %}
}
